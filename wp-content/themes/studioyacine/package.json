{
  "name": "studioyacine",
  "version": "1.0.0",
  "main": "gulpfile.js",
  "copyright": "studioyacine",
  "author": "Yacine Belarbi, studioyacine",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@codewhore.com:nystudio107.git"
  },
  "urls": {
    "live": "https://haelixa.com",
    "local": "haelixa.local"
  },
  "vars": {
    "siteCssName": "site.combined.min.css",
    "scssName": "style.scss",
    "cssName": "style.css"
  },
  "paths": {
    "src": {
      "css": "./library/css/",
      "fonts": "./library/fonts/",
      "js": "./library/js/",
      "img": "./library/img/",
      "scss": "./library/scss/"
    },
    "build": {
      "css": "./build/css/",
      "fonts": "./build/fonts/",
      "js": "./build/js/",
      "html": "./build/html/",
      "img": "./build/img/"
    },
    "dist": {
      "css": "./dist/css/",
      "js": "./dist/js/",
      "fonts": "./dist/fonts/",
      "img": "./dist/img/"
    },
    "scss": [],
    "templates": "./"
  },
  "globs": {
    "babelJs": [
      "./library/js/*.js"
    ],
    "buildJs": [
      "./build/js/*.js"
    ],
    "distCss": [
      "./node_modules/normalize.css/normalize.css",
      "./node_modules/flickity/css/flickity.css",
      "./build/css/*.css"
    ],
    "distJs": [
      "./node_modules/jquery/dist/jquery.js",
      "./node_modules/@barba/core/dist/barba.js",
      "./node_modules/vanilla-lazyload/dist/lazyload.js",
      "./node_modules/gsap/src/uncompressed/TweenMax.js",
      "./node_modules/gsap/src/uncompressed/easing/EasePack.js",
      "./node_modules/gsap/src/uncompressed/plugins/ScrollToPlugin.js",
      "./node_modules/flickity/dist/flickity.pkgd.js"
    ]
  },
  "dependencies": {
    "@barba/core": "^2.4.0",
    "flickity": "^2.2.0",
    "gsap": "^2.0.2",
    "jquery": "^3.3.1",
    "modularscale-sass": "^3.0.8",
    "normalize.css": "^8.0.1",
    "sass-math-pow": "^0.1.5",
    "vanilla-lazyload": "^10.19.1"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "browser-sync": "^2.25",
    "chalk": "^1.1.3",
    "fancy-log": "^1.3.3",
    "git-rev-sync": "^1.7.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-filter": "^5.0.0",
    "gulp-header": "^1.8.7",
    "gulp-if": "^2.0.1",
    "gulp-load-plugins": "^1.3.0",
    "gulp-newer": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "moment": "^2.14.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
